{
    "name": "designbycode/laravel-helpers",
    "description": "Simple laravel helper utilities",
    "keywords": ["laravel-helper", "menu active", "active class", "body class", "set classes"],
    "license": "MIT",
    "authors": [
        {
            "name": "Claude Myburgh",
            "email": "claude@deisgnbycode.co.za"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "illuminate/support": "^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "DesignByCode\\LaravelHelpers\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "DesignByCode\\LaravelHelpers\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "laravel": {
            "providers": [
                "DesignByCode\\LaravelHelpers\\Providers\\HelpersServiceProvider"
            ],
            "aliases": {
                "Set": "DesignByCode\\LaravelHelpers\\Facades\\Set"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
